    "@babel/preset-env": "^7.7.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^4.2.0",
    "jest": "^25.2.2",
    "node-sass": "^5.0.0",
    "raw-loader": "^3.1.0",
